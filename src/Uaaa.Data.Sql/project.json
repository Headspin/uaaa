{
  "version": "1.1.0",
  "title": "Uaaa Data Sql Extensions",
  "copyright": "© 2017 Dvabita",
  "authors": [
    "DvaBita"
  ],
  "description": "Uaaa Framework data extensions for SQL",

  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "System.Collections.Immutable": "1.2.0",
    "System.Data.SqlClient": "4.1.0",
    "Uaaa.Core": "2.0.*"
  },

  "frameworks": {
    "netstandard1.6": {
      "imports": "dnxcore50",
      "dependencies": {
        "System.Linq": "4.1.0"
      }
    },
    "net46": {
      "dependencies": {
        "System.Linq": "4.0.0.0"
      }
    }
  },

  "buildOptions": {
    "warningsAsErrors": true,
    "xmlDoc": true,
    "debugType": "portable"
  },

  "packOptions": {
    "owners": [ "DvaBita " ],
    "licenseUrl": "https://raw.githubusercontent.com/Headspin/uaaa/master/LICENSE",
    "requireLicenseAcceptance": true,
    "releaseNotes": "UpdateQuery builder now supports fields filtering."
  }
}